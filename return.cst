fn main() as i32 {
    let is i32 as i with mut = 0;
    while (i < 10) {
        i = i + 1;
        if (i == 5) {
            continue;
        }
        if (i == 8) {
            break;
        }
    }
    
    let is i32 as result with mut = 0;

    if (1 == 1 || 1 == 0) {
        result = result + 1;
    }
    
    if (1 == 0 && 1 == 1) {
        result = result + 100;
    }
    
    return i + result; // Should be 8 + 1 = 9
}

use "linux.cst";
use "string.cst";
use "mem.cst";

fn print(s as String) {
    if (s.len > 0) {
        write(STDOUT, s.ptr, cast(i64, s.len));
    }
}

fn println(s as String) {
    print(s);
    write(STDOUT, "\n", 1);
}

fn print_int(n as i64) {
    let is String as s = int_to_string(n);
    print(s);
    string_free(s);
}
